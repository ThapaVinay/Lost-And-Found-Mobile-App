{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "lost-and-found",
    "slug": "lost-and-found",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "31fff813-092f-484a-b7ad-43cb40aa049a"
      }
    },
    "android": {
      "package": "com.toxino.lostandfound",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "owner": "aakshita"
  }
}
